{
  "name": "mocklets",
  "version": "0.0.6",
  "description": "Reusable standard mocks and fakes for popular browser and Node.js APIs, framework/library objects for Jest",
  "homepage": "https://github.com/codesplinta/mocklets",
  "main": "dist/mocklets.cjs.js",
  "module": "dist/mocklets.es.js",
  "typings": "./types/index.d.ts",
  "files": [
    "examples",
    "dist",
    "types",
    ".npmignore",
    "CHANGELOG.md",
    "LICENSE",
    "package-lock.json",
    "README.md"
  ],
  "keywords": [
    "jest",
    "testing",
    "mocks",
    "fakes",
    "resuable"
  ],
  "scripts": {
    "lint": "standard --fix",
    "test": "echo \"test\"",
    "build:rollup": "rollup -c rollup.config.js",
    "cleanup": "rimraf dist",
    "build": "npm run cleanup && npm run build:rollup"
  },
  "engines": {
    "node": ">=8.0.0 <19.0.0"
  },
  "author": {
    "name": "Ifeora Okechukwu <isocroft@gmail.com>",
    "url": "https://isocroft.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/codesplinta/mocklets.git"
  },
  "bugs": "https://github.com/codesplinta/mocklets/issues",
  "license": "Apache-2.0",
  "standard": {
    "globals": [
      "global",
      "jest",
      "process",
      "beforeAll",
      "afterAll",
      "beforeEach",
      "afterEach"
    ],
    "ignore": [
      "/examples",
      "/.github",
      "/test",
      "rollup.config.js"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.14.1",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-strip": "^3.0.4",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/mock-fs": "4.13.4",
    "jest": "^26.6.3",
    "mock-fs": "5.2.0",
    "prettier": "^1.16.4",
    "rimraf": "^3.0.2",
    "rollup": "^2.47.0",
    "standard": "^16.0.4",
    "timekeeper": "2.3.1"
  },
  "dependencies": {
    "@types/mock-fs": "4.13.4",
    "mock-fs": "5.2.0",
    "timekeeper": "2.3.1"
  }
}
