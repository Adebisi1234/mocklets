{
  "name": "mocklets",
  "version": "0.0.1",
  "description": "Reusable standard mocks and fakes for popular browser APIscand Node.js framework/library objects for Jest",
  "homepage": "https://github.com/codesplinta/mocklets",
  "main": "dist/mocklets.cjs.js",
  "browser": "dist/mocklets.umd.js",
  "bin": ".bin/mocklets",
  "workspaces": [
    "packages/*"
  ],
  "files": [
    "dist",
    ".bin",
    "CHANGELOG.md",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "jest",
    "testing",
    "mocks",
    "fakes",
    "resuable"
  ],
  "scripts": {
    "lint": "standard --fix",
    "build": "lerna run build --ignore mockelets",
    "watch": "lerna run watch --parallel",
    "bootstrap": "lerna bootstrap",
    "prepublish": "lerna run build --ignore mockelets",
    "test": "echo \"test\""
  },
  "author": {
    "name": "Ifeora Okechukwu <isocroft@gmail.com>",
    "url": "https://isocroft.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/codesplinta/mocklets.git"
  },
  "bugs": "https://github.com/codesplinta/mocklets/issues",
  "license": "Apache-2.0",
  "standard": {
    "globals": [
      "global",
      "jest"
    ],
    "ignore": [
      "/.github",
      "/tests"
    ]
  },
  "directories": {
    "test": "test"
  },
  "devDependencies": {
    "standard": "^16.0.4",
    "jest": "^26.6.3",
    "jasmine": "^4.5.0",
    "lerna": "^3.21.0",
    "rimraf": "^2.6.3",
    "prettier": "^1.16.4"
  },
  "dependencies": {}
}
